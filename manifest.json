{
"update_url": "https://google.co.in",

    "manifest_version": 2,

    "name": "Noun Search",
    "version": "1.0",

    "permissions": [
        "tabs", "http://*/*"
    ],

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*", "file:///*"],
            "css": ["content.css"],
            "js": ["jquery.min.js", "content.js"]
        }
    ],

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": {
            "19": "icon19.png",
            "38": "icon38.png"
        }
    },

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
